#include<stdio.h>
struct student
{
	char name[50];
	int roll;
	float marks;
} s[5];
int main()
{
	int i;
		printf("enter information of students:\n");
		for(i=0; i<5; ++i)
		{
			s[i].roll=i+1;
			printf("\nFor roll %d,\n",s[i].roll);
			printf("enter name:");
			scanf("%S",s[i]. name);
			printf("enter marks:");
			scanf("%f",&s[i].marks);
		}
		printf("displaying the information:\n\n");

		for(i=0; i<5; ++i)
		{
			printf("\nFor roll %d,\n",i+1);
			scanf("name");
			puts(s[i]. name);
			printf("marks:%.if",s[i].marks);
			printf("\n");
		}
		return 0;
}
